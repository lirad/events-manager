<p id="notice"><%= notice %></p>

<h1>Event Atendees</h1>

<table>
  <thead>
    <tr>
      <th>Event</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_atendees.each do |event_atendee| %>
      <tr>
        <td><%= event_atendee.event_id %></td>
        <td><%= event_atendee.user_id %></td>
        <td><%= link_to 'Show', event_atendee %></td>
        <td><%= link_to 'Edit', edit_event_atendee_path(event_atendee) %></td>
        <td><%= link_to 'Destroy', event_atendee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event Atendee', new_event_atendee_path %>
